{
  "name": "middle-earth-agents-api",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "description": "Agents server for Middle Earth Agents",
  "main": "src/app.ts",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/app.ts",
    "build": "tsup",
    "start": "node dist/app.js",
    "clean": "node -e \"fs.rmSync('./dist', { recursive: true, force: true }); fs.rmSync('./coverage', { recursive: true, force: true });\"",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "tsx --test 'src/**/*.test.ts'",
    "test:watch": "tsx --test --watch 'src/**/*.test.ts'",
    "test:coverage": "tsx --test --coverage 'src/**/*.test.ts'",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "prisma db push --force-reset && prisma generate && prisma db seed"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.2",
    "@anthropic-ai/sdk": "0.34.0-alpha.0",
    "@coral-xyz/anchor": "^0.30.1",
    "@prisma/client": "^6.2.1",
    "@privy-io/server-auth": "^1.18.4",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/ws": "^8.5.14",
    "ai": "^4.1.2",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "express-csp-header": "^6.0.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "js-sha1": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "natural": "^6.12.0",
    "node-cache": "^5.1.2",
    "openai": "^4.83.0",
    "prisma": "^6.2.1",
    "twitter-api-v2": "^1.19.0",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/chai": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.16",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
